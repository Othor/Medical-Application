<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    android:background="@android:color/white"
    android:icon="@mipmap/c">

    <ImageView
        android:layout_width="113dp"
        android:layout_height="49dp"
        android:id="@+id/imageView"
        android:layout_weight="0.21"
        android:src="@mipmap/f"
        android:layout_x="93dp"
        android:layout_y="323dp"
        android:onClick="neutrition"
        android:layout_below="@+id/imageView2"
        android:layout_toEndOf="@+id/textView"
        android:layout_marginStart="40dp" />
    <ImageView
        android:layout_width="113dp"
        android:layout_height="49dp"
        android:id="@+id/imageView3"
        android:layout_weight="0.21"
        android:src="@mipmap/c"
        android:layout_x="91dp"
        android:layout_y="220dp"
        android:onClick="appenedHistory"
        android:layout_above="@+id/imageView"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="113dp"
        android:layout_height="49dp"
        android:id="@+id/imageView2"
        android:layout_weight="0.21"
        android:src="@mipmap/g"
        android:layout_x="95dp"
        android:layout_y="115dp"
        android:layout_above="@+id/btn_ambulance"
        android:layout_toEndOf="@+id/textView2"
        android:layout_marginBottom="42dp"
        android:onClick="healthScore" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/textView2"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="113dp"
        android:layout_height="49dp"
        android:id="@+id/btn_ambulance"
        android:layout_weight="0.21"
        android:src="@mipmap/ambulance"
        android:layout_x="93dp"
        android:layout_y="412dp"
        android:onClick="ambulance"
        android:layout_above="@+id/textClock"
        android:layout_toEndOf="@+id/textView2"
        android:layout_marginBottom="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="My Health Score"
        android:id="@+id/textView3"
        android:layout_x="91dp"
        android:layout_y="173dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Insert History"
        android:id="@+id/textView4"
        android:layout_x="93dp"
        android:layout_y="276dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nutrition Tip?"
        android:id="@+id/textView5"
        android:layout_x="93dp"
        android:layout_y="369dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Call Ambulance"
        android:id="@+id/textView6"
        android:layout_x="93dp"
        android:layout_y="449dp" />

    <AbsoluteLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#15668f">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Abebe Kefyalew"
            android:id="@+id/textView7"
            android:layout_gravity="center_horizontal"
            android:layout_x="125dp"
            android:layout_y="13dp"
            android:textColor="#ffffff"
            android:autoText="false" />

        <ImageView
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_gravity="center"
            android:src="@mipmap/ph"
            android:layout_marginRight="150dp"
            android:layout_marginTop="10dp"
            android:layout_x="25dp"
            android:layout_y="15dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Extension Officer"
            android:id="@+id/textView8"
            android:layout_x="125dp"
            android:layout_y="42dp"
            android:textColor="#ffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Active"
            android:id="@+id/textView9"
            android:layout_x="130dp"
            android:layout_y="69dp"
            android:textColor="#0816cd" />

        <TextClock
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textClock"
            android:textColor="#ffffff"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="45dp"
            android:layout_x="180dp"
            android:layout_y="69dp" />

    </AbsoluteLayout>

</AbsoluteLayout>
