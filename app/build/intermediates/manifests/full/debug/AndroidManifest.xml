<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jamie.autosearch_test1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.jamie.autosearch_test1.app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/c"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.jamie.autosearch_test1.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.jamie.autosearch_test1.Login" />
        <activity android:name="com.example.jamie.autosearch_test1.MainActivity" />
        <activity android:name="com.example.jamie.autosearch_test1.HomeActivity" />
        <activity android:name="com.example.jamie.autosearch_test1.PatientHistory" />
        <activity android:name="com.example.jamie.autosearch_test1.ProfileActivity" />
        <activity android:name="com.example.jamie.autosearch_test1.HealthScoreActivity" />
        <activity android:name="com.example.jamie.autosearch_test1.ScodeDetailsActivity" />
        <activity android:name="com.example.jamie.autosearch_test1.RegistrationActivity" />
    </application>

</manifest>