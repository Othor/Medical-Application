<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <AbsoluteLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="83dp"
        android:background="#15668f">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Patient Health Score"
            android:id="@+id/textView7"
            android:layout_gravity="center_horizontal"
            android:layout_x="89dp"
            android:layout_y="30dp"
            android:textColor="#ffffff"
            android:autoText="false" />


    </AbsoluteLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Kassa Mohammad"
        android:id="@+id/textView9"
        android:layout_gravity="center_horizontal"
        android:layout_x="143dp"
        android:layout_y="95dp"
        android:textColor="#000000"
        android:autoText="false" />

    <ImageView
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:layout_gravity="center"
        android:src="@mipmap/ph"
        android:layout_marginRight="150dp"
        android:layout_marginTop="10dp"
        android:layout_x="25dp"
        android:layout_y="91dp" />

    <ImageView
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:id="@+id/imageView4"
        android:layout_gravity="center_horizontal"
        android:layout_x="6dp"
        android:layout_y="238dp"
        android:src="@drawable/score_bar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="70"
        android:id="@+id/textView10"
        android:layout_x="107dp"
        android:layout_y="343dp"
        android:textSize="50dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:id="@+id/button2"
        android:layout_x="19dp"
        android:layout_y="173dp"
        android:onClick="profile" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Details"
        android:id="@+id/button3"
        android:layout_x="200dp"
        android:layout_y="379dp"
        android:onClick="scoreDetail" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="29"
        android:id="@+id/textView11"
        android:layout_x="141dp"
        android:layout_y="151dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Male"
        android:id="@+id/textView14"
        android:layout_x="139dp"
        android:layout_y="122dp" />
</AbsoluteLayout>